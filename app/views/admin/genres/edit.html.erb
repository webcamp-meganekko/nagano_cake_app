<div class="row">
  <h1>ジャンル編集</h1>
  <div class="col">
    <%= form_with model: @genre, url: admin_genre_path, local:true do |f| %>
    <div class="form-group">
      <%= f.label :genre_name%>
      <%= f.text_field :genre_name, class: 'form-control-plaintext genre_name' %>
    </div>
    <div class="form-group">
      <%= f.submit '変更を保存', class: 'form-control-plaintext btn btn-success' %>
    </div>
    <% end %>
  </div>
</div>
